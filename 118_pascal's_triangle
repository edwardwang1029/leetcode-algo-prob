class Solution(object):
    def generate(self, numRows):
        """
        :type numRows: int
        :rtype: List[List[int]]
        """
        root = []
        if numRows == 0:
            return root
        n = 1
        while n >= 1 and n <= numRows:
            root.append([1] * n)
            for i in range(1, len(root[n-1])-1):
                root[n-1][i] = root[n-2][i-1] + root[n-2][i]
            n += 1
        return root            
            
